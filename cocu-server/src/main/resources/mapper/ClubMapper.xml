<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cocu.mapper.ClubMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.cocu.entity.Club">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="avatar" property="avatar" />
        <result column="description" property="description" />
        <result column="creator_id" property="creatorId" />
        <result column="president_id" property="presidentId" />
        <result column="type_id" property="typeId" />
        <result column="create_date" property="createDate" />
        <result column="update_date" property="updateDate" />
        <result column="is_recommend" property="isRecommend" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, avatar, description, creator_id, president_id, type_id, create_date, update_date
    </sql>

</mapper>
